  "apidoc": {
    "title": "College Rest api Doc",
    "url": "https://0.0.0.0:10000/api/v1"
  },
  "scripts": {
    "dev": "nodemon --watch \"src/**/*\" --ext \"ts .env json\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node src/server.ts\"",
    "build": "tsc -p .",
    "start": "npm run build && NODE_ENV=prud node dist/server.js  ",
    "g:apidoc": "npx apidoc -i src/ -o apidoc/ -t ./node_modules/apidoc-plugin-websocket/template",
    "g:migration": "npx sequelize-cli migration:generate --name",
    "migrate": "npx sequelize-cli db:migrate",
    "migration:revert": "npx sequelize-cli db:migrate:undo"
  },
